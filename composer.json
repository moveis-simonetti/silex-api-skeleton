        }
    ],
    "require": {
        "php": ">=7.1",
        "silex/silex": "^2.0",
        "monolog/monolog": "^1.11.0",
        "dflydev/doctrine-orm-service-provider": "^2.0",
        "doctrine/dbal": "~2.5",
        "doctrine/orm": "~2.5",
        "vlucas/phpdotenv": "^2.4",
        "kurl/silex-doctrine-migrations-provider": "^0.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "doctrine/data-fixtures": "^1.2"
    },
    "autoload": {
        "psr-4": {
